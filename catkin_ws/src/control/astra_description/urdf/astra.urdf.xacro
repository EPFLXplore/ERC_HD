	<!-- ==========LINKS========== -->


	<!-- base_link -->
	<link name="base_link">
		<visual>
			<geometry>
				<box size="${base_link_dim}"/>
			</geometry>
			<origin xyz="${base_link_origin}" />
		</visual>
		<collision>
			<geometry>
				<box size="${base_link_dim}"/>
			</geometry>
			<origin xyz="${base_link_origin}" />
		</collision>
	</link>

	<!-- link1 -->
