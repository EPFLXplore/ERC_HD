	<!-- ==========JOINTS========== -->


	<!-- joint1 -->
	<joint name="joint1" type="revolute">
		<parent link="base_link"/>
		<child link="link1"/>
		<origin xyz="${base_link_attach_point}" />
		<axis xyz="0 0 1"/>
		<limit effort="10" velocity="0.2" lower="${(pi*-2)}" upper="${(pi*2)}"/>
	</joint>

	<!-- joint2 -->
	<joint name="joint2" type="revolute">
		<parent link="link1"/>
		<child link="link2"/>
		<origin xyz="${link1_attach_point}" />
		<axis xyz="0 1 0"/>
		<limit effort="10" velocity="0.2" lower="-1.34" upper="2.07"/>
	</joint>

	<!-- motor_j3_joint -->
	<joint name="motor_j3_joint" type="fixed">
		<parent link="link2"/>
		<child link="motor_j3"/>
		<origin xyz="${link2_attach_point}" />
		<axis xyz="1 0 0"/>
		<limit effort="1" velocity="0.2" lower="${-(pi)}" upper="${pi}"/>
	</joint>

	<!-- joint3 -->
	<joint name="joint3" type="revolute">
		<parent link="link2"/>
		<child link="link3"/>
		<origin xyz="${link2_attach_point}" rpy="0 ${pi/2} 0" />
		<axis xyz="0 1 0"/>
		<limit effort="10" velocity="0.2" lower="-0.7" upper="0.6"/>
	</joint>

	<!-- joint4 -->
	<joint name="joint4" type="revolute">
		<parent link="link3"/>
		<child link="link4"/>
		<origin xyz="${link3_attach_point}" />
		<axis xyz="0 0 1"/>
		<limit effort="10" velocity="0.2" lower="${(pi/-2)}" upper="${(pi/2)}"/>
	</joint>

	<!-- joint5 -->
	<joint name="joint5" type="revolute">
		<parent link="link4"/>
		<child link="link5"/>
		<origin xyz="${link4_attach_point}" />
		<axis xyz="0 1 0"/>
		<limit effort="10" velocity="0.2" lower="-1.9" upper="1.9"/>
	</joint>

	<!-- joint6 -->
	<joint name="joint6" type="revolute">
		<parent link="link5"/>
		<child link="gripper_base"/>
		<origin xyz="${link5_attach_point}" />
		<axis xyz="0 0 1"/>
		<limit effort="10" velocity="0.2" lower="${-(pi)}" upper="${pi}"/>
	</joint>

	<!-- gripper_left_finger_joint -->
	<joint name="gripper_left_finger_joint" type="prismatic">
		<parent link="gripper_base"/>
		<child link="gripper_left_finger"/>
		<origin xyz="${gripper_base_attach_point}" />
		<axis xyz="0 1 0"/>
		<limit effort="1" velocity="0.5" lower="0" upper="0.04"/>
	</joint>

	<!-- gripper_right_finger_joint -->
	<joint name="gripper_right_finger_joint" type="prismatic">
		<parent link="gripper_base"/>
		<child link="gripper_right_finger"/>
		<origin xyz="${gripper_base_attach_point}" />
		<axis xyz="0 1 0"/>
		<limit effort="1" velocity="0.5" lower="0" upper="0.1"/>
		<mimic joint="gripper_left_finger_joint" multiplier="-1" offset="0"/>
	</joint>




</robot>